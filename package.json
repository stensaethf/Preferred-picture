{
  "name": "preferred-picture",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "seed": ". ./seeds/refresh_db.sh",
    "dev": "PREFERREDPICTUREDEV=1 gulp & nodemon -e js ./bin/www",
    "startmongo": "mongod -f ./mongoconf.yml",
    "mongo": "mongo -u preferredpictureprod -p 'preferredpicture1!' localhost:27017/preferredpicture",
    "mongoadmin": "mongo -u preferredpictureadmin -p 'preferredpicture1!' localhost:27017/admin",
    "restartmongo": "killall mongod && npm run startmongo"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "connect-mongo": "^0.7.0",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "~1.3.5",
    "crypto": "0.0.3",
    "debug": "^2.2.0",
    "express": "~4.4.5",
    "express-session": "^1.7.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-if": "^2.0.0",
    "gulp-jade": "^1.1.0",
    "gulp-jshint": "^2.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "jade": "~1.11.0",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "merge-stream": "^1.0.0",
    "mongoose": "^4.0.8",
    "morgan": "~1.6.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "passport": "^0.2.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "through2": "^2.0.1",
    "uglify-js": "^2.6.2"
  },
  "description": "Node webapp for comparing and ranking pictures",
  "main": "app.js",
  "keywords": [
    "comparison",
    "ranking",
    "picture",
    "node",
    "webapp"
  ],
